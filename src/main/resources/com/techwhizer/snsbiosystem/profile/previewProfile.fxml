<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Pagination?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.techwhizer.snsbiosystem.controller.profile.PreviewProfile">
   <center>
      <TableView fx:id="tableview" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="colClientId" maxWidth="75.0" minWidth="75.0" text="CLIENT ID" />
           <TableColumn fx:id="colUsername" maxWidth="180.0" minWidth="180.0" text="USERNAME" />
           <TableColumn fx:id="colFirstName" maxWidth="180.0" minWidth="170.0" text="FIRST NAME" />
           <TableColumn fx:id="colLastName" maxWidth="180.0" minWidth="170.0" text="LAST NAME" />
           <TableColumn fx:id="colOfficeName" maxWidth="200.0" minWidth="190.0" text="OFFICE NAME" />
           <TableColumn fx:id="colEmail" maxWidth="230.0" minWidth="210.0" text="EMAIL" />
           <TableColumn fx:id="colTelephoneNum" maxWidth="180.0" minWidth="180.0" text="TELEPHONE NUM" />
           <TableColumn fx:id="colStatus" minWidth="120.0" text="STATUS" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </center>
   <bottom>
      <VBox prefHeight="90.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <HBox>
               <children>
                  <Pagination fx:id="pagination" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
            <HBox style="-fx-border-color: grey;">
               <children>
                  <HBox spacing="20.0">
                     <children>
                        <HBox alignment="CENTER" fillHeight="false">
                           <children>
                              <Label minWidth="117.0" prefWidth="117.0" text="TOTAL RECORD :">
                                 <font>
                                    <Font name="Arial Bold" size="13.0" />
                                 </font>
                              </Label>
                              <Label fx:id="totalRecordL" minWidth="20.0" textFill="#030ffc">
                                 <font>
                                    <Font name="Arial Black" size="13.0" />
                                 </font>
                              </Label>
                           </children>
                           <HBox.margin>
                              <Insets left="10.0" />
                           </HBox.margin>
                        </HBox>
                        <Separator orientation="VERTICAL" />
                        <HBox alignment="CENTER" layoutX="10.0" layoutY="10.0">
                           <children>
                              <Label minWidth="157.0" prefHeight="16.0" prefWidth="157.0" text="TOTAL VALID RECORD :">
                                 <font>
                                    <Font name="Arial Bold" size="13.0" />
                                 </font>
                              </Label>
                              <Label fx:id="totalValidRecordL" minWidth="20.0" textFill="#e44210">
                                 <font>
                                    <Font name="Arial Bold" size="13.0" />
                                 </font>
                              </Label>
                           </children>
                        </HBox>
                        <Separator orientation="VERTICAL" />
                        <HBox alignment="CENTER" layoutX="168.0" layoutY="10.0">
                           <children>
                              <Label minWidth="171.0" prefHeight="16.0" prefWidth="171.0" text="TOTAL INVALID RECORD :">
                                 <font>
                                    <Font name="Arial Bold" size="13.0" />
                                 </font>
                              </Label>
                              <Label fx:id="totalInvalidRecordL" minWidth="20.0" textFill="#fc0000">
                                 <font>
                                    <Font name="Arial Bold" size="13.0" />
                                 </font>
                              </Label>
                           </children>
                        </HBox>
                        <Separator layoutX="392.0" layoutY="10.0" orientation="VERTICAL" prefHeight="200.0" />
                     </children>
                     <HBox.margin>
                        <Insets bottom="10.0" />
                     </HBox.margin>
                  </HBox>
                  <HBox alignment="CENTER" fillHeight="false" prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
                     <children>
                        <HBox alignment="CENTER" spacing="50.0">
                           <children>
                              <Button mnemonicParsing="false" onAction="#cancelBnClick" prefHeight="26.0" prefWidth="139.0" style="-fx-background-color: red; -fx-background-radius: 3;" text="CANCEL" textFill="WHITE">
                                 <font>
                                    <Font name="System Bold" size="13.0" />
                                 </font>
                              </Button>
                              <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#changeFileBnClick" prefHeight="26.0" prefWidth="139.0" style="-fx-background-color: #6600cc; -fx-background-radius: 3;" text="CHANGE FILE" textFill="WHITE">
                                 <font>
                                    <Font name="System Bold" size="13.0" />
                                 </font>
                              </Button>
                              <HBox alignment="CENTER">
                                 <children>
                                    <Button fx:id="uploadNowBn" mnemonicParsing="false" onAction="#uploadBnClick" prefHeight="27.0" prefWidth="162.0" style="-fx-background-color: #006666; -fx-background-radius: 3;" text="UPLOAD NOW" textFill="WHITE">
                                       <font>
                                          <Font name="System Bold" size="13.0" />
                                       </font>
                                    </Button>
                                    <ProgressIndicator fx:id="progressbar" minHeight="35.0" minWidth="35.0" prefHeight="35.0" prefWidth="35.0" />
                                 </children>
                              </HBox>
                           </children>
                           <HBox.margin>
                              <Insets top="5.0" />
                           </HBox.margin>
                        </HBox>
                     </children>
                     <HBox.margin>
                        <Insets bottom="10.0" />
                     </HBox.margin>
                  </HBox>
               </children>
            </HBox>
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </VBox>
   </bottom>
</BorderPane>
